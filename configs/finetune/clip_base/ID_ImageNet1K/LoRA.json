{
    "data_root": "/data/DATASETS/SVD_OOD",
    "id_dataset": "ID_ImageNet1K",
    "split": "val",
    "batch_size": 100,
    "seed": 5,
    "device": "cuda",
    "scorers": ["mcm_score", "gl_mcm_score"],
    "temperature": 100,
    "exp_name": "LoRA",
    "log_directory": "./results/finetune/clip_base/{args.id_dataset}/{args.exp_name}",
    "recall_level": 0.95,
    "model_name": "openai/clip-vit-base-patch16",
    "model_type": "CLIP",
    "num_train_epochs": 5,
    "gradient_accumulation_steps": 1,
    "learning_rate": 0.01,
    "weight_decay": 0.0005,
    "momentum": 0.9,
    "lr_scheduler_type": "cosine",
    "num_warmup_steps": 0,
    "locoop_lambda": 0.1,
    "locoop_top_k": 300,
    "lora_svd_init": false,
    "lora_r": 32,
    "lora_alpha": 16,
    "target_modules": ["mlp.fc1"]
}